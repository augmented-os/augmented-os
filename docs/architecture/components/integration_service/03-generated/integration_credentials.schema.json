{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "integration_credentials",
  "title": "IntegrationCredentials",
  "type": "object",
  "properties": {
    "accessToken": {
      "type": "string",
      "minLength": 1
    },
    "refreshToken": {
      "type": "string",
      "minLength": 1
    },
    "expiresAt": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "accessToken"
  ],
  "additionalProperties": false
}